{
  "name": "mlint",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://dig.cmu.edu/eyeintoai",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "again": "^0.0.1",
    "classnames": "^2.2.6",
    "dom-to-image": "^2.6.0",
    "dom-to-image-font-patch": "^1.0.1",
    "dotenv": "^8.1.0",
    "dotenv-webpack": "^7.1.0",
    "firebase": "^8.6.8",
    "i": "^0.3.7",
    "npm": "^8.4.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.2",
    "react-router": "5.1.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^5.0.0",
    "react-with-firebase-auth": "^1.2.0",
    "sass": "^1.49.7",
    "webpack": "^5.68.0"
  },
  "scripts": {
    "start": "PORT=8080 react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -t -r https://github.com/cmudig/eyeintoai.git -d build"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "block-spacing": "error",
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "comma-spacing": "error",
      "comma-style": "error",
      "curly": "error",
      "default-case": "error",
      "handle-callback-err": "error",
      "indent": [
        "error",
        2
      ],
      "jsx-quotes": "error",
      "no-console": "off",
      "no-empty-function": "error",
      "no-floating-decimal": "error",
      "no-inline-comments": "off",
      "no-lonely-if": "error",
      "no-multi-spaces": "error",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxEOF": 1,
          "maxBOF": 0
        }
      ],
      "no-trailing-spaces": "error",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prefer-const": "error",
      "quotes": [
        "error",
        "single"
      ],
      "react/no-unescaped-entities": "off",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off",
      "semi": "error",
      "space-before-blocks": "error",
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "space-in-parens": "error",
      "space-infix-ops": "error",
      "space-unary-ops": [
        "error",
        {
          "words": true,
          "nonwords": false
        }
      ],
      "spaced-comment": "error",
      "yoda": "error",
      "eol-last": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "sass": "^1.49.7"
  }
}